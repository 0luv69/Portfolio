{
  "builds": [
    {
      "src": "protfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.2" }
    }
  ],
  "routes": [
        {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "protfolio/wsgi.py"
    }
  ]
}

